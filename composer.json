    "autoload": {
        "psr-4": {
            "Brain\\Cell\\": "src/Brain/Cell"
        }
    },
    "require": {
        "php": ">=7.0",

        "doctrine/collections": "~1.0",
        "doctrine/common": "~2.0",

        "symfony/http-foundation": "~3.1 || ~4.0",
        "symfony/validator": "~3.1 || ~4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",

        "friendsofphp/php-cs-fixer": "2.8.3",
        "mmoreram/php-formatter": "^1.3",

        "guzzlehttp/guzzle": "^6.2"
    },
    "suggest": {
        "guzzlehttp/guzzle": "Allow requests to be made using the GuzzleHttpRequestAdapter"
    }
}
